{
    "name": "hollow3464/azure-oauth-helper",
    "description": "Azure Oauth Helper",
    "type": "library",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Hollow3464\\AzureOauthHelper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Chamorro",
            "email": "daved3464@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "lint": "vendor/bin/phpstan"
    },
    "require": {
        "guzzlehttp/psr7": "^2.4",
        "league/oauth2-client": "^2.7",
        "symfony/cache": "^6.1",
        "thenetworg/oauth2-azure": "^2.2"
    },
    "require-dev": {
        "laravel/pint": "^1.15.1",
        "phpstan/phpstan": "^1.10.67"
    }
}
